{
	"name": "Server Vigil Agent",
	"dockerComposeFile": "../docker-compose.yml",
	"service": "agent",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}/agent",
	"customizations": {
		"vscode": {
			"extensions": [
				"streetsidesoftware.code-spell-checker",
				"streetsidesoftware.code-spell-checker-german"
			]
		}
	},
	"remoteUser": "root",
	"mounts": [
		{
			"source": "${devcontainerId}-cargo-cache",
			"target": "/usr/local/cargo",
			"type": "volume"
		},
		{
			"source": "${devcontainerId}-target",
			"target": "${containerWorkspaceFolder}/target",
			"type": "volume"
		}
	],
	"shutdownAction": "none"
}